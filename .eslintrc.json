{
    "root": true,
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
      // "eslint:recommended",
      "google"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": [
          "warn"
        ],
        "prefer-arrow-callback": [
          "warn"
        ],
        "no-invalid-this": [
          "warn"
        ]
    }
}
